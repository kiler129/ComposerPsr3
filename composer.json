{
  "name": "noflash/composer_psr3",
  "description": "PSR-3 complaint logger for use with Composer IOInterface",
  "minimum-stability": "beta",
  "license": "MIT",
  "authors": [
    {
      "name": "Grzegorz Zdanowski",
      "email": "grzegorz@noflash.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/log": "~1.0",
    "composer/composer": "~1.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6.0"
  },
  "autoload": {
    "psr-4": {
      "noFlash\\ComposerPsr3\\": "src"
    }
  },
  "provide": {
    "psr/log-implementation": "1.0.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
